{
  "extends": [
    "fullstack",
    "airbnb",
    "prettier/react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "impliedStrict": true,
      "classes": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true,
    "jest": true
  },
  "rules": {
    "no-debugger": 0,
    "no-unused-vars": [
      0, // 1,
      {
        "argsIgnorePattern": "res|next|^err"
      }
    ],
    "arrow-body-style": [
      1, // 2,
      "as-needed"
    ],
    "no-unused-expressions": [
      1, // 2,
      {
        "allowTaggedTemplates": true
      }
    ],
    "no-param-reassign": [
      1, // 2,
      {
        "props": false
      }
    ],
    "no-console": 0,
    "import/prefer-default-export": 0,
    "import": 0,
    "func-names": 0,
    "comma-dangle": 0,
    "max-len": 0,
    "import/extensions": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "react/display-name": 1,
    "react/react-in-jsx-scope": 0,
    "react/prefer-stateless-function": 0,
    "react/forbid-prop-types": 0,
    "react/no-unescaped-entities": 0,
    "jsx-a11y/accessible-emoji": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "radix": 0,
    "no-shadow": [
      1, // 2,
      {
        "hoist": "all",
        "allow": [
          "resolve",
          "reject",
          "done",
          "next",
          "err",
          "error"
        ]
      }
    ],
    "quotes": [
      1,
      "single",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ],
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": [
      "warn",
      {
        "aspects": [
          "invalidHref"
        ]
      }
    ],
    "space-before-function-paren": [ // function (), not function()
      1,
      "always"
    ],
    "arrow-parens": [
      1,
      "as-needed"
    ],
    "prefer-const": 1,
    "object-curly-spacing": 1,
    "no-plusplus": 0, // i.e. allow i++, not just i += 1
    "no-restricted-properties": 0, // i.e. alllow Math.pow(), not just **
    "prefer-destructuring": 0,
    "react/prop-types": 0,
    "import/no-unresolved": 0,
    "jsx-a11y/alt-text": 0,
    "jsx-a11y/img-redundant-alt": 0,
    "class-methods-use-this": 0,
    "import/first": 0,
    "react/no-unused-state": 0,
    "no-useless-constructor": 0,
    "jsx-a11y/label-has-for": 0,
    "strict": 0,
    "spaced-comment": 0,
    "one-var": 0,
    "react/sort-comp": 0,
    "import/no-extraneous-dependencies": 0
  }
}
